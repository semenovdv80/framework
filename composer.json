{
  "name": "semenovdv80/framework",
  "description": "The SemenovDV80/MVC Framework.",
  "version": "1.0.0",
  "keywords": ["framework", "semenovdv80"],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitriy Semenov",
      "email": "semenovdv80@gmail.com",
      "homepage": "http://github.com/semenovdv80/framework",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "nesbot/carbon": "1.25.*"
  },
  "replace": {
    "essential/http": "self.version"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "files": [
      "src/Essential/Basement/helpers.php"
    ],
    "psr-4": {
      "Essential\\": "src/Essential/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Essential\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
